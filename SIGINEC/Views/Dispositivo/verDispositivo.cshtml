@model Model.Dispositivo

@{
    ViewBag.Title = "verDispositvo";
    Layout = null;
}

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    <h4 class="modal-title" style="text-align:center;">
        Detalles Dispositivo
        <i class="glyphicon glyphicon-eye-open"></i>
    </h4>
</div>
<div class="modal-body">
    <fieldset disabled>
        <div class="form-group">
            <label> Nombre: </label>
            @Html.TextBoxFor(d => d.Nombre, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label> Clase: </label>
            @Html.TextBoxFor(d => d.Clase, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label> Marca: </label>
            @Html.TextBoxFor(d => d.Marca, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label> Clase: </label>
            @Html.TextBoxFor(d => d.Clase, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label> Modelo: </label>
            @Html.TextBoxFor(d => d.Modelo, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label> Caracteristicas: </label>
            @Html.TextAreaFor(d => d.Caracteristicas, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label> Configuración: </label>
            @Html.TextAreaFor(d => d.Configuracion, new { @class = "form-control" })
        </div>
    </fieldset>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
</div>